{
  "name": "robo-dex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "build": "rm -rf .next && rm -rf export && rm -rf out && next build",
    "build:staging": "rm -rf .next && rm -rf export && rm -rf out && blitz build -e staging",
    "export": "yarn run build && next export",
    "export:staging": "yarn run build:staging && blitz export -e staging",
    "start": "next start",
    "deploy": "aws --region us-east-1 s3 sync ./out s3://app.roboglobal.info --delete",
    "cache": "aws cloudfront create-invalidation --distribution-id=E19OS8EKLBFXJ9 --paths '/*'",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:style": "npx stylelint '**/*.{ts,tsx}'",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check:staged": "lint-staged",
    "prepare": "husky install",
    "updateLPsAPR": "yarn turbo run build --filter=@pancakeswap/sdk --filter=@pancakeswap/farm --filter=@pancakeswap/token-lists && NODE_PATH=./apps/web/src ts-node --project ./apps/web/tsconfig.json --compiler-options '{\"module\":\"commonjs\"}' scripts/updateLPsAPR.ts",
    "typechain": "typechain --out-dir src/config/abis/types --target=ethers-v5 \"src/config/abis/abi/*.json\""
  },
  "dependencies": {
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/address": "5.7.0",
    "@ethersproject/bignumber": "5.7.0",
    "@ethersproject/constants": "5.7.0",
    "@ethersproject/contracts": "5.7.0",
    "@ethersproject/providers": "5.7.2",
    "@next/bundle-analyzer": "13.0.0",
    "@popperjs/core": "2.11.6",
    "@reduxjs/toolkit": "1.8.6",
    "@sentry/nextjs": "7.80.0",
    "@uniswap/permit2-sdk": "1.2.0",
    "@uniswap/smart-order-router": "2.10.2",
    "@uniswap/token-lists": "1.0.0-beta.30",
    "@uniswap/universal-router-sdk": "1.3.9",
    "@wagmi/core": "0.10.6",
    "@web3-react/core": "6.1.9",
    "ajv": "8.12.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-http": "1.5.17",
    "big.js": "6.2.1",
    "bignumber.js": "9.1.0",
    "blitz": "2.0.0-beta.35",
    "date-fns": "2.29.3",
    "dayjs": "1.11.8",
    "decimal.js-light": "2.5.1",
    "ethers": "5.7.2",
    "graphql-tag": "2.12.6",
    "i18next": "22.0.4",
    "jotai": "1.9.0",
    "jsbi": "3.2.0",
    "lightweight-charts": "3.8.0",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "ms.macro": "2.0.0",
    "next": "13.0.0",
    "next-i18next": "12.1.0",
    "next-transpile-modules": "10.0.0",
    "numbro": "2.3.6",
    "numeral": "2.0.6",
    "react": "18.2.0",
    "react-device-detect": "2.2.3",
    "react-dom": "18.2.0",
    "react-fast-marquee": "1.3.5",
    "react-i18next": "12.0.0",
    "react-popper": "2.3.0",
    "react-redux": "8.0.4",
    "react-transition-group": "4.4.5",
    "react-window": "1.8.8",
    "recharts": "2.4.3",
    "redux-persist": "6.0.0",
    "styled-components": "5.3.6",
    "styled-system": "5.1.5",
    "swr": "1.3.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "toformat": "2.0.0",
    "typechain": "8.1.0",
    "viem": "1.0.7",
    "wagmi": "0.12.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.0.0",
    "@svgr/webpack": "6.5.0",
    "@typechain/ethers-v5": "10.1.0",
    "@types/lodash": "4.14.186",
    "@types/ms.macro": "2.0.0",
    "@types/node": "18.11.6",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "@types/react-redux": "7.1.24",
    "@types/styled-components": "5.1.26",
    "@types/styled-system": "5.1.15",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "babel-plugin-macros": "3.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "6.0.0",
    "lint-staged": "13.2.1",
    "prettier": "2.7.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-styled-components": "0.1.1",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@uniswap/sdk-core": "3.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn check:type"
    }
  }
}
